<script type="text/javascript">
window.addEventListener('load', function () {
  if (!/aliexpress\.ru$/.test(window.location.hostname)) {
    return;
  }
  var targetBlocks = [
    '.top-lighthouse',
    '.me-header',
    '.navigation',
    '.me-ui-box',
    '.site-footer .sf-siteIntro',
    '.site-footer .sf-seoKeyword',
    'footer-copywrite'
  ];
  var targetLinkPat = /aliexpress\.com/;
  var targetDestLink = 'aliexpress.ru';
  var links = [];
  targetBlocks.forEach(function (block) {
    var arr = [].slice.call(document.querySelectorAll(block + ' a'));
    links = links.concat(arr);
  });
  links.forEach(function (link) {
    if (targetLinkPat.test(link.href)) {
      link.href = link.href.replace(targetLinkPat, targetDestLink);
    }
  });
  var nationBlock = '.site-footer .sf-MultiLanguageSite';
  var nationLinkPat = /ru\.aliexpress\.com/;
  var nationLinks = [].slice.call(document.querySelectorAll(nationBlock + ' a'));
  nationLinks.forEach(function (link) {
    if (nationLinkPat.test(link.href)) {
      link.href = link.href.replace(nationLinkPat, 'ru.aliexpress.ru');
    }
  });
});
</script>
