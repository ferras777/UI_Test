<script charset="utf-8" src="//g.alicdn.com/alime/dialog/alicare-dialog.js"></script>
<script type="text/javascript">
    try {
    	(function () {
    		var from, configDom, isUndraggable;
    		var position = {};
    		var isMatchConfig = false;
    		window.setTimeout(function () {
    			if (window.AlicareDialog) {
    				hasMatch();
    				console.log(isMatchConfig, 'isMatchConfig',position);
    				if (isMatchConfig) {
    				    var search = window.location.search;
    				    var orderId = getParam(search, 'orderId');
    					var ad = new AlicareDialog({
    						requestHost: 'https://alimebot.aliexpress.com',
    						botUrl: 'https://alimebot.aliexpress.com/intl/index.htm',
    				        // botUrl: 'https://pre-ai.alimebot.aliexpress.com/intl/index.htm',
    						position: position,
    						// [必填] 在后台注册的页面名称
    						from: from,
    						// [可选] 默认小蜜内部滚动时不冒泡给外部，如需获取小蜜内部滚动事件，设置 true
    						activeScroll: false,
    						// [可选] 配置服务窗是否可拖动，false 为可拖动，true 为不可拖动，默认为 false。
    						undraggable: isUndraggable,
    						// [可选] 服务窗的 z-index 值，可选，默认为999999
    						'z-index': 999999,
    						orderId: orderId ? orderId : ''
    
    					});
    
    					ad.onRendered(function () {
    						var xiaoMiDialog = document.querySelector('#J_xiaomi_dialog');
    						var header = xiaoMiDialog.querySelector('.alime-strong-header');
    						var jWeak = xiaoMiDialog.querySelector('.J_weak');
    						if (header) {
    							header.querySelector('img').setAttribute('src', '//ae01.alicdn.com/kf/H794f99266c5d4fd387a337d99a786172K.png');
    							header.querySelector('img').style.display = 'block';
    						}
    						if (jWeak) {
    							jWeak.querySelector('img').setAttribute('src', '//ae01.alicdn.com/kf/H794f99266c5d4fd387a337d99a786172K.png');
    						}
    						xiaoMiDialog.querySelector('.close-icon').addEventListener("click", function () {
    							var weak = xiaoMiDialog.querySelector('.J_weak');
    							weak.querySelector('img').setAttribute('src', '//ae01.alicdn.com/kf/H794f99266c5d4fd387a337d99a786172K.png');
    						});
    						
    					});
    				}
    			}
    		}, 1000)
    
    		// 从cookie里面获取语种
    		function regGetCookie(key) {
    			var cookie = document.cookie;
    			if (cookie.indexOf(key) === -1) return '';
    			var reg = new RegExp('.*([^;&]*' + key + '=)(.*?)([;&]|$).*');
    			return cookie.replace(reg, '$2')
    		}
    
    		// 查询配置url和语言是否与当前页面和语言匹配
    		function hasMatch() {
    			var alimeItems = document.querySelectorAll('.alime-item');
    			var url = window.location.host + window.location.pathname;
    			// 获取语种
    			var locale = regGetCookie('x_locale').substr(3) || 'US';
    			alimeItems.forEach(function (link, index) {
    				if (link.getAttribute('data-url').indexOf(url) > -1) {
    					var langItems = link.querySelectorAll('.alime-lang');
    					langItems.forEach(function (item) {
    						if (item.getAttribute('data-lang') === locale) {
    							configDom = item;
    							isMatchConfig = true;
    							getConfig();
    							return;
    						}
    					});
    				}
    			})
    		}
    
    		// 获取配置信息
    		function getConfig() {
    			alimeContext = configDom.querySelector('.alime-context');
    			from = alimeContext.getAttribute('data-from');
    			isUndraggable = alimeContext.getAttribute('data-undraggable');
    			var positionArr = alimeContext.getAttribute('data-position').split(",");
    			positionArr.forEach(function (item, index) {
    				var parseItem = parseInt(item);
    				if (index == 0 && parseItem && parseItem !== 0) {
    					position.top = parseItem;
    				} else if (index == 1 && parseItem && parseItem !== 0) {
    					position.right = parseItem;
    				} else if (index == 2 && parseItem && parseItem !== 0) {
    					position.bottom = parseItem;
    				} else if (index == 3 && parseItem && parseItem !== 0) {
    					position.left = parseItem;
    				}
    			});
    		}
    		
    		// 获取订单号
    		function getParam (str, key) {
                if (!str || !key) return;
                str = decodeURI(str);
                var regStr = new RegExp('(' + key + '=)(.*?)([;&]|$)');
                if (str.indexOf(key) !== -1) {
                    return str.match(regStr)[2];
                }
            }
    
    	})()
    }
    catch (err) {
    	console.log(err);
    }</script>
